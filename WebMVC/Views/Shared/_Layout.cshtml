    <script src="@Url.Content("~/Scripts/vendor/jquery.nice-select.min.js")"></script>
    <script src="@Url.Content("~/Scripts/vendor/jquery.sticky.js")"></script>
    <script src="@Url.Content("~/Scripts/vendor/nouislider.min.js")"></script>
    <script src="@Url.Content("~/Scripts/vendor/jquery.magnific-popup.min.js")"></script>
    <script src="@Url.Content("~/Scripts/vendor/owl.carousel.min.js")"></script>
    <!-- gmaps Js -->
    <script src="@Url.Content("~/Scripts/gmaps.min.js")"></script>
    <script src="@Url.Content("~/Scripts/main.js")"></script>
    @RenderSection("Scripts", required: false)
